  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Webpack Start",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "devWebpack"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Server Start",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "devServer"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
  ]
}